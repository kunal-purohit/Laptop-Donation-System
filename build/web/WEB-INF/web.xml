<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>StudentRegServlet</servlet-name>
        <servlet-class>com.team4.servlet.StudentRegServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DonorRegServlet</servlet-name>
        <servlet-class>com.team4.servlet.DonorRegServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LaptopServlet</servlet-name>
        <servlet-class>com.team4.servlet.LaptopServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.team4.servlet.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminServlet</servlet-name>
        <servlet-class>com.team4.servlet.AdminServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentRegServlet</servlet-name>
        <url-pattern>/StudentRegServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DonorRegServlet</servlet-name>
        <url-pattern>/DonorRegServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LaptopServlet</servlet-name>
        <url-pattern>/LaptopServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminServlet</servlet-name>
        <url-pattern>/AdminServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
